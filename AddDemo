/*public abstract boolean add(E): this method can add element in collection
and if element is added return true otherwise return false */



import java.util.*;

class AddDemo{
	
	public static void main(String x[]){
		
		ArrayList<Integer> al = new ArrayList<>();
		boolean b = al.add(10);
		
		if(b){
			
			System.out.println("Element added successfully");
		}else{
			
			System.out.println("Some problem is there");
			
		}
	}
}